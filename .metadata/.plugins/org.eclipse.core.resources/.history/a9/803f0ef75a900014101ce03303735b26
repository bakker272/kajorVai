
public class RecursiveMethod {

	public static void main(String[] args) {
		String a="asdf";
		String []b=new String[a.length()];
		for(int i=a.length()-1;i>=0;i--)
		{
			b[i]=a.substring(i);
			System.out.print(b[i]);
		}
		
	}
	
	public static int reusable(int i){
		
		
		return reusable(i-1);
	}

}
